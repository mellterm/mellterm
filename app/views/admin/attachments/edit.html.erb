<h2><%= @title = "Edit File" %></h2>

<% form_for @attachment, :url => { :action => "update" }, :html => { :multipart => true } do |f| %>


  <%= f.error_messages %>
  
  <%= render :partial => 'form', :locals => { :f => f, :item => @item } %>
  
  <p>
    <%= f.submit "Save" %>
    <input type="reset" value="reset" >
  </p>

<% end %>

<%= link_to "Back", admin_attachments_path %>

