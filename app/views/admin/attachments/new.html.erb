<h2><%= @title = "Upload a File" %></h2>

<% form_for(@attachment, :url => admin_attachments_path, :html => { :multipart => true }) do |f| %>

  <%= f.error_messages %>
  
  <%= render :partial => 'form', :locals => { :f => f, :item => @item } %>
  
  <p>
    <%= f.submit "Upload" %>
    <input type="reset" value="Reset">
  </p>

<% end %>

<%= link_to "Back", admin_attachments_path %>

